<form id="register">
  <div class="mb-3">
    <label for="username" class="form-label">ชือผู้ใช้</label>
    <input
      type="text"
      autocomplete="off"
      class="form-control"
      id="username"
      aria-describedby="emailHelp"
    />
  </div>


  <div class="mb-3">
    <label for="password" class="form-label">Password</label>
    <input
      type="password"
      autocomplete="off"
      class="form-control"
      id="password"
    />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
<script type="module" src="./register.js"></script>
