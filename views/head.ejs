<div id="userset" data-user="<%= JSON.stringify(user) %>">
  <%= user.name.username %>
</div>
<%-include("nav1.ejs")%> <%-include("tos.ejs")%>
<br /><br /><br /><br />
<form id="fmadd" class="d-flex">
  <input
    class="form-control me-2"
    type="text"
    id="head"
    placeholder="เพิ่ม"
    aria-label="Add new"
    autocomplete="off"
  />
  <button class="btn btn-outline-success" type="submit">เพิ่ม</button>
</form>
<div id="heads">Heads</div>
<div
  class="modal fade"
  id="exampleModal"
  tabindex="-1"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">หัวข้อรายการ</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <form>
          <div class="mb-3">
            <label for="message-text" class="col-form-label">หัวข้อ:</label>
            <textarea class="form-control" id="message-text"></textarea>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button
          type="button"
          id="btnCancle"
          class="btn btn-secondary"
          data-bs-dismiss="modal"
        >
          ยกเลิก
        </button>
        <button type="button" id="btnSave" class="btn btn-primary">
          บันทึก
        </button>
      </div>
    </div>
  </div>
</div>
<script type="module" src="./heads.js"></script>
